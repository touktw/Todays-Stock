<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_home"
    app:startDestination="@id/stockListFragment">

    <fragment
        android:id="@+id/stockListFragment"
        android:name="club.qwer.stock.screen.home.list.StockListFragment">
        <action
            android:id="@+id/stockListToStockDetail"
            app:destination="@id/stockDetailFragment"
            app:enterAnim="@anim/nav_default_pop_enter_anim"
            app:exitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>

    <fragment
        android:id="@+id/stockDetailFragment"
        android:name="club.qwer.stock.screen.home.detail.StockDetailFragment" />
</navigation>